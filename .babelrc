{
  "presets": [["@babel/env", { "modules": false }],"@babel/react"],
  "plugins": ["@babel/plugin-transform-modules-commonjs", "@babel/plugin-proposal-class-properties",[
    "@babel/plugin-transform-runtime",
    {
      "helpers": true,
      "regenerator": true,
      "useESModules": true
    }
  ],[
    "add-module-exports",
    {
      "addDefaultProperty": true
    }
  ]]
}
